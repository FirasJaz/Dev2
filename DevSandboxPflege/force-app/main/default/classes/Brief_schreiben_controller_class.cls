    public PageReference processBriefSchreiben() {       
        // Empänger
        if(empf == null) {
            empf = new Brief_schreiben_adresse(Kunde.id);
            
            selRCPT = empf.selectRcpt();
            
            adr.empAnrede = empf.AddressRecord.empAnrede;
            // adr.empName = empf.AddressRecord.empName;
            //adr.empStrasse = empf.AddressRecord.empStrasse;
            adr.empOrt = empf.AddressRecord.empOrt;
            adr.empPLZ = empf.AddressRecord.empPLZ; 
            //adr.empMail = empf.AddressRecord.empMail;           
                   
            adr.empName = empf.AddressRecord.empName;
            adr.empStrasse = empf.AddressRecord.empStrasse;
            adr.empMail = empf.AddressRecord.empMail;
            
            
            
            if(adr.empMail == null)
            {
            hasMail = true;
            }
            else {
            hasMail = false;
            }
                        
            selectedRcpt = empf.getSelectedRcpt();
            
        }
        
        try {
            selectedRcpt = ApexPages.currentPage().getParameters().get('Rcpt');
            this.changeRcpt();
            }
            catch (System.exception e) {}
        
        // CompanyInfo
        FirmenInfo = CompanyInfo__c.getValues('Default');
        if (FirmenInfo != null) {
            if(FirmenInfo.GF__c.contains(', ')){
                String[] GF = FirmenInfo.GF__c.split(', ',2);
                FirmenInfo.GF__c = GF[0] + '<br />'+GF[1];
            }
        }

        if(etid != null) {
            mth = new mail_template_holen(Kunde.id, id.valueOf(etid));
            system.debug('############### Alex200 ' + tempBodyHTML);    
            schreiben = mth.getPlaintextBody();
            htmlt = mth.getHTMLbody();
            betreff = mth.getBetreff();
        }
        else {
            mth = new mail_template_holen(Kunde.id);
            tempBodyHTML = mth.getHTMLbody();
            tempBodyText = mth.getPlaintextBody();
            tempSubject = mth.getBetreff();
        }    
        return null;
    }
    
    
