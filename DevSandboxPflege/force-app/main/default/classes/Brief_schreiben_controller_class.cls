    public void createTask() {
        // TaskStatus ts = [SELECT MasterLabel FROM TaskStatus WHERE IsClosed = true];
        Task th = new Task(ActivityDate = Date.today(),
                            Subject='Brief versandt',
                            WhatId = Kunde.Id,
                            Description = 'Brief mit folgendem Inhalt versandt:\n\n'
                                        +'Betreff:\n'+betreff+'\n\n'
                                        +'Schreiben:\n'+schreiben,
                            OwnerId = UserInfo.getUserId(),
                            Status='Geschlossen');
        insert th;
    }
    
    public void createTaskEmail() {
        Task th1 = new Task(ActivityDate = Date.today(),
                            Subject='Brief per Email versandt',
                            WhatId = Kunde.Id,
                            Description = 'Brief per Email versandt an ' + empf.getEmpfaenger() + ' (' + adr.empMail + ') Betreff:\n' +betreff+'\n\n'+'Schreiben:\n'+schreiben,
                            OwnerId = UserInfo.getUserId(),
                            Status='Geschlossen');
        insert th1;
    }
    
    /**
     * redirects to visualforcepage CustomEmailTemplateSelect and
     * sets current url as parameter (see CustomEmailTemplateSelect)
     *
     * (does not use currentPage.getUrl() because value is to long
     *  and contains unnecessary information)
     */
    public PageReference GotoTemplateSelect()
    {
        PageReference pg = new PageReference('/apex/CustomEmailTemplateSelect');
        String returnUrlStr= '/apex/Brief_schreiben_Form?id=' + ApexPages.currentPage().getParameters().get('id');
        pg.getParameters().put('returnURL', returnUrlStr);
        pg.getParameters().put('Rcpt', selectedRcpt);
        pg.setRedirect(true);
        return pg;
    }
    
