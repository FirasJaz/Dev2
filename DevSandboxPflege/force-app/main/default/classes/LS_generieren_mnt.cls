    public ApexPages.StandardSetController setCon {
        get{
            if(setCon == null){
                size = 100;
                if(lsType == 'mon') {
                    setCon = createCont_Mon();
                    buttonLabel = 'LS generieren';
                    pageTitle = 'Bedarf'; 
                }
                if(lsType == 'pg51') {
                    setCon = createCont_pg51();
                    buttonLabel = 'LS generieren';
                    pageTitle = 'Täglich. Produktgruppe 51.'; 
                    lsSpalte = true;
                }   
                if(lsType == 'pg54') {
                    setCon = createCont_pg54();
                    buttonLabel = 'LS generieren';
                    pageTitle = 'Jetzt liefern.'; 
                    lsSpalte = true;
                }  
                if(lsType == 'inko') {
                    setCon = createCont_inko();
                    buttonLabel = 'LS generieren';
                    pageTitle = 'Täglich. Inko-Artikel.'; 
                    lsSpalte = true;
                }               
                if(lsType == 'sam') {
                    setCon = createCont_sam();
                    buttonLabel = 'LS generieren';
                    pageTitle = 'Sammelbestellung. Produktgruppe 54.'; 
                    lsSpalte = true;
                }                       
                setCon.setPageSize(size);
                noOfRecords = setCon.getResultSize();
        
                if((retMsg != null) && (retMsg != '')) {
                    ApexPages.addMessage(new ApexPages.message(ApexPages.severity.INFO, retMsg));    
                }
                
            }
            return setCon;
        }set;
    }
    
