    public pageReference createLead() {
        boolean error = false;
        integer c = 0;
        set<string> arNamen = new set<string>();
        id rtid = null;                                
        list<RecordType> rtList = [SELECT Id FROM RecordType WHERE sObjectType='lead' AND Name = 'Shop'];
        if((rtList != null) && (rtList.size() > 0)) {
            rtId = rtList[0].id;
        }
        
        list<Lead_Position__c> lpList = new list<Lead_Position__c>(); 
        
        // AM 26.10.2016
        id cmpId;
        List<Campaign> cmplist = [SELECT id, name FROM Campaign WHERE name = 'SHOP' LIMIT 1];        
        if(cmplist != null && cmplist.size() != 0) {
            cmpId = cmplist[0].Id;
        }
                    
                                    
        
        for(string inc : incIdset) {
            if(c >= maxLeadsProClick) break;
            magento.salesOrderEntity soe = ms.salesOrderInfo(sessionID, inc);
            lead ld = new lead(company = 'Magento Shop', RecordTypeID = rtid, OM_Wizzard_Name2__c = cmpId);
            ld.Salutation = soe.customer_prefix;
            ld.lastname = soe.customer_lastname;
            ld.firstname = soe.customer_firstname;
            system.debug('######################### wds 003 Anrede=  ' + soe.customer_prefix);
            system.debug('######################### wds 006 Anrede=  ' + soe.customer_lastname);
            system.debug('######################### wds 004 Anrede=  ' + ld.salutation);
            ld.magento_kundennummer__c = soe.customer_id;
            ld.email = soe.customer_email;
            ld.magento_increment_id__c = inc;
            ld.magento_order_id__c = soe.order_id;
            ld.Magento_payment_method__c = soe.payment.method;
            magento.salesOrderAddressEntity shipping_address = soe.shipping_address;
            magento.salesOrderAddressEntity billing_address = soe.billing_address;
            ld.shipping_street__c = shipping_address.street;
            ld.shipping_postal_code__c = shipping_address.postcode;
            ld.shipping_city__c = shipping_address.city;
            ld.shipping_country__c = shipping_address.country_id;
            try {
                ld.shipping_firstname__c = zMap.get(inc).shipping_firstname;
                ld.shipping_lastname__c = zMap.get(inc).shipping_lastname;
                ld.shipping_name__c = zMap.get(inc).shipping_name;
                ld.billing_firstname__c = zMap.get(inc).billing_firstname;
                ld.billing_lastname__c = zMap.get(inc).billing_lastname;
                ld.billing_name__c = zMap.get(inc).billing_name;
                system.debug('######################### alex005 ld.billing_name__c=' + ld.billing_name__c + ' ld.shipping_name__c=' + ld.shipping_name__c);
            }
            catch (System.Exception e) {
            }
            ld.billing_street__c = billing_address.street;
            ld.billing_postal_code__c = billing_address.postcode;
            ld.billing_city__c = billing_address.city;
            ld.billing_country__c = billing_address.country_id;        
            
            insLead.add(ld);
            for(integer p=0; p< 1000; p++) {
                try {
                /**
                 if ean is "see next ean", position is only dummy-position from magento, because it's a configurable item
                 and followed by product with correct ean, so don't create lead-position for dummy
                 attention: price, tax_percent and original_price are 0 in position with correct ean;
                 @todo 9.10.15 ms check if price, tax and ori-price are actually needed in lead-position?!
                 12.10.2015 wds habe auf 1000 erhöht, da er die Positionen sonst nicht alle abholt!
                 */
                    if(soe.items.complexObjectArray[p].ean != 'see next ean')
                    {
                        Lead_Position__c lp = new Lead_Position__c();
                        lp.Order_Id__c = soe.items.complexObjectArray[p].order_id;
                        lp.name__c = soe.items.complexObjectArray[p].name;
                        lp.item_Id__c = soe.items.complexObjectArray[p].item_id;
                        lp.Price__c = soe.items.complexObjectArray[p].price;
                        lp.Qty_Ordered__c = soe.items.complexObjectArray[p].qty_ordered;
                        lp.SKV__c = soe.items.complexObjectArray[p].sku;
                        lp.EAN__c = soe.items.complexObjectArray[p].ean;
                        lp.Tax_Percent__c = soe.items.complexObjectArray[p].tax_percent;
                        lp.product_id__c = soe.items.complexObjectArray[p].product_id;
                        lp.Original_Price__c = soe.items.complexObjectArray[p].Original_Price;
                        lpList.add(lp);
                        arNamen.add(lp.name__c);
                    }
                    
                }
                catch (System.Exception e) {
                    break;
                }
            }  
            c++;
        }
        
        if(!insLead.isEmpty()) {
            insert insLead;
        }
        map<string, id> ldidMap = new map<string, id>();
        map<string, string> eanMap = new map<string, string>();
        list<lead> newLeads = [SELECT id, magento_order_id__c FROM lead WHERE magento_increment_id__c IN : incIdset];
        if((newLeads != null) && (newLeads.size() > 0)) {
            for(lead ld : newLeads) ldidMap.put(ld.magento_order_id__c, ld.id);
        }
        list<Artikel__c> arList = [SELECT name, id, a_pac_apo_EAN_UPC__c FROM Artikel__c WHERE name IN : arNamen];
        if((arList != null) && (arList.size() > 0)) {
            for(Artikel__c ar : arList) {
                eanMap.put(ar.name, string.valueOf(ar.a_pac_apo_EAN_UPC__c)); 
            }
        }
        
        
        if(!lpList.isEmpty()) {
            for(Lead_Position__c lp : lpList) {
                if(ldidMap.containsKey(lp.Order_Id__c)) lp.Lead__c = ldidMap.get(lp.Order_Id__c); 
                if(eanMap.containsKey(lp.name__c)) {
                    if(lp.EAN__c == null) {
                        lp.EAN__c = eanMap.get(lp.name__c);
                    }
                }
            }
            insert lpList;
        }
        
        
        //return null;
        PageReference pr = new PageReference('/apex/Invoke_magento');
        pr.setRedirect(true);
        return pr;
        
    }
    
    public pageReference reloadSeite() {
        PageReference pr = new PageReference('/apex/Invoke_magento');
        // PageReference pr = new PageReference('/apex/magent_reload');
        pr.setRedirect(true);
        return pr;      
    }    
    
