    public invoke_magento() {
        // create set of already in SF existing
        integer anzLeads = 0;
        integer maxAu = 1000;
        set<string> incSetExist = new set<string>();
        list<lead> existLead = [SELECT magento_increment_id__c FROM lead where magento_increment_id__c != null];
        if((existLead != null) && (existLead.size() > 0)) {
            for(lead ld : existLead) incSetExist.add(ld.magento_increment_id__c);
            anzLeads = incSetExist.size();
        }
        maxAu = maxAu - anzLeads;
        if(maxAu > 1) {
            list<Auftrag__c> existAu = [SELECT Magento_increment_id__c FROM Auftrag__c 
                                        WHERE Magento_increment_id__c != null
                                        ORDER BY CreatedDate DESC LIMIT : maxAu];
            if((existAu != null) && (existAu.size() > 0)) {
                for(Auftrag__c au : existAu) incSetExist.add(au.Magento_increment_id__c);
            }       
        }
        ms.timeout_x = 60000;
        sessionID = ms.login('movedata', 'xpP748yZGxwdy6YXwPcn');
        magento.salesOrderListEntityArray solea = ms.salesOrderList(sessionId, filters);
        // @todo 09.10.2015 check why i<10 before and improve logic (?)
        zeilen.clear();
        for (integer i=0; i<420;i++) {
            try {
                magento.salesOrderListEntity sole = solea.complexObjectArray[i];
                string incrementID = sole.increment_id;
                if(!incSetExist.contains(incrementID)) { 
                    
                    zeile z = new zeile();
                    z.incrID = incrementID;
                    z.Salutation = sole.customer_prefix;
                    system.debug('######################### wds 001 Anrede=  ' + sole.customer_prefix);
                    system.debug('######################### wds 002 Anrede=  ' + z.salutation);
                    z.status = sole.status;
                    z.lname = sole.customer_lastname;
                    z.fname = sole.customer_firstname;
                    z.email = sole.customer_email;
                    z.shipping_name = sole.shipping_name;
                    z.shipping_firstname = sole.shipping_firstname;
                    z.shipping_lastname = sole.shipping_lastname;
                    z.billing_name = sole.billing_name;
                    z.billing_firstname = sole.billing_firstname;
                    z.billing_lastname = sole.billing_lastname;
                    z.crDate = sole.created_at;
                    z.udDate = sole.updated_at;
                    if(sole.status == 'processing') {
                        incIdset.add(incrementID);
                        zProc.add(z);
                    }
                    else {
                        zOther.add(z);
                    }
                    // zeilen.add(z);
                    zMap.put(z.incrID, z);
                    } else fertige++;
                    
                    
                }
                catch (System.Exception e) { 
                    //ApexPages.addMessage(new ApexPages.message(ApexPages.severity.ERROR, 'Fehler beim Senden callout salesOrderListEntity ' + e)); 
                    break; 
                }               
        }
                    if((zProc != null) && (zProc.size() > 0)) {
                        for(zeile z : zProc) zeilen.add(z);
                    }
                    if((zOther != null) && (zOther.size() > 0)) {
                        for(zeile z : zOther) zeilen.add(z);
                    }    
                    pending = zOther.size();
                    processing = zProc.size();
                    totalrecord = zeilen.size();
    }   

