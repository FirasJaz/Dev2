    public static void update_Artikel(List<Artikel__c> ArtOld, 
                                       List<Artikel__c> ArtNew) {
        boolean error  = false;                                 
        for(Integer i = 0; i < ArtNew.size(); i++) { 
            if (ArtNew.get(i).AEK_F__c != ArtOld.get(i).AEK_F__c) {
                error = update_AEK(ArtNew.get(i).Id, ArtNew.get(i).AEK_F__c);
            }
            if (ArtNew.get(i).EEK__c != ArtOld.get(i).EEK__c) {
                error = update_EEK(ArtNew.get(i).Id, ArtNew.get(i).EEK__c);
            }
            if (ArtNew.get(i).EVK_netto__c != ArtOld.get(i).EVK_netto__c) {
                error = update_EVK(ArtNew.get(i).Id, ArtNew.get(i).EVK__c);
            }
            if (ArtNew.get(i).AVK__c != ArtOld.get(i).AVK__c) {
                error = update_AVK(ArtNew.get(i).Id, ArtNew.get(i).AVK__c);
            }
        }
    }
    
    public static Boolean update_AEK(ID ArtID, Decimal newPreis) {
        boolean error = false;
        Decimal newNettoPreis = 0;
        List<Preislisteneintrag__c> prList = [
        SELECT Preis_netto__c, Preis_faktor__c, Preis_berechnungsart__c
        FROM Preislisteneintrag__c
        WHERE Preisgrundlage__c = 'AEK' 
        AND Auslauf_datum__c = null
        AND Artikel__c =: ArtID
        ];
        if (prList != null && prList.size() != 0){
            for(Integer p = 0; p < prList.size(); p++) {
                newNettoPreis = rechnen_netto_preis(newPreis, prList.get(p).Preis_faktor__c, prList.get(p).Preis_berechnungsart__c);    
                prList.get(p).Preis_netto__c = newNettoPreis;
            }   
            try {
                    update prList;                               
                }  
            catch (system.dmlexception d) {
                    error = True;
                    // system.debug('############# update Preislisteneintrag error: '+d);
                }           
        }
        return error;
    }  
    
