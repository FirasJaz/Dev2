    private static Decimal rechnen_netto_preis(Decimal basisPreis, Decimal Faktor, String art) {
        Decimal newNettoPreis = 0;
        if (art == 'Prozent') {
            newNettoPreis = basisPreis + basisPreis/100 * Faktor;       
        }   
        else {
            newNettoPreis = basisPreis + Faktor;        
        }       
        return newNettoPreis;
    }                                

