        public RA_nur_anzeigen_sto_controller_class(String Kunde_id) {       
                Kunde = [select Name, Anrede__c, Vorname__c, Name__c, 
                                Geburtsdatum__c, Versichertennummer__c, 
                                Krankenkasse__c, komb_Adresse__c, komb_name__c,
                                Ort__c, PLZ__c, Stra_e__c, Hausnummer__c,
                                Kundennummer__c, Krankenkasse__r.Name
                                from Kunde__c Where ID = :Kunde_id];

                KK_id = Kunde.Krankenkasse__c ;
                
                KK = [select Name, BillingCity, BillingStreet, BillingPostalCode 
                                from Account Where ID = :KK_id]; 
        IF (Kunde.Anrede__c == Null) {
            Anredeformel = ' ';
        }
        else {                          
        If (Kunde.Anrede__c.contains('Herr') ) {
                        
                        Anredeformel = 'Sehr geehrter Herr ';     
            system.debug ('wds1 Kunden.Anrede ' + Kunde.Anrede__c);                  
                } 
        If (Kunde.Anrede__c.contains('Frau') ) {
                        
                        Anredeformel = 'Sehr geehrte Frau '; 
                        system.debug ('wds2 Kunden.Anrede ' + Kunde.Anrede__c);    
                      
                }         
        }
        
        }
