public with sharing class delivery_tracking {

    public Integer noOfRecords{get; set;}
    public Integer size{get;set;}
    public integer counter=0;  //keeps track of the offset
    public integer list_size=20; //sets the page size or number of rows
    public integer total_size; //used to show user the total size of the list  
    
    private list<Lieferbest_tigung__c> LBlist = new List<Lieferbest_tigung__c>();
    public class tZeile {
        public String LsNr {get;set;}
        public ID LsID {get;set;}
        public String Kunde {get;set;}
        public ID KdID {get;set;}
        public String Attach {get;set;}
        public ID AtID {get;set;}   
        public String Rechnung {get;set;}
        public ID RgID {get;set;}   
        public ID GnID {get;set;}
        public string Gen {get;set;}
        public boolean anAbena {get;set;}
        public boolean vonAbena {get;set;}
        public boolean anAsb {get;set;}
        public boolean unterschrift {get;set;}
    }
    public list<tZeile> tZeilen = new List<tZeile>();
    
    public delivery_tracking() {
    }
    
    public ApexPages.StandardSetController setCon {
        get{
            if(setCon == null){
                size = list_size;
                setCon = new ApexPages.StandardSetController(Database.getQueryLocator(
                    [SELECT id, Name, Kunde__c, Kunde__r.Name, Anhang__c, 
                            Lieferschein__c, 
                            Lieferschein__r.Name,
                            Lieferschein__r.Unterschrift__c,
                            Lieferschein__r.Genehmigung__c,
                            Lieferschein__r.Genehmigung__r.Name,
                            PDFName__c,
                            an_asb_versendet__c,
                            an_abena_versendet__c,
                            abena_empfang__c,
                            Rechnung__c,
                            Rechnungsname__c
                        FROM Lieferbest_tigung__c
                        WHERE pdf_erstellt__c != true
                        ORDER BY Lieferschein__r.Name DESC]));
                setCon.setPageSize(size);
                noOfRecords = setCon.getResultSize();
            }
            return setCon;
        }set;
    }
    
    public List<tZeile> gettZeilen() {
        tZeilen.Clear();
        LBlist.Clear();
        for(Lieferbest_tigung__c LB : (List<Lieferbest_tigung__c>)setCon.getRecords()) { 
            LBlist.add(LB);
        }   
        if((LBlist != null) && (LBlist.size() > 0)) {
            for(Lieferbest_tigung__c LB : LBlist) {
                tZeile tdZeile = new tZeile();
                tdZeile.AtID = id.valueOf(LB.Anhang__c);
                tdZeile.Attach = LB.PDFName__c;
                tdZeile.LsNr = LB.Lieferschein__r.Name;
                tdZeile.LsID = LB.Lieferschein__c;               
                tdZeile.Kunde = LB.Kunde__r.Name;
                tdZeile.KdID = LB.Kunde__c;  
                tdZeile.GnID = LB.Lieferschein__r.Genehmigung__c;
                tdZeile.Gen = LB.Lieferschein__r.Genehmigung__r.Name;
                tdZeile.anAbena = LB.an_abena_versendet__c;
                tdZeile.vonAbena = LB.abena_empfang__c;
                tdZeile.anAsb = LB.an_asb_versendet__c;
                tdZeile.Rechnung = LB.Rechnungsname__c;
                tdZeile.RgId = LB.Rechnung__c;
                tdZeile.unterschrift = LB.Lieferschein__r.Unterschrift__c;
                tZeilen.add(tdZeile);                               
            }
        }
        return tZeilen;
    }

    public Boolean hasNext {
        get {
            return setCon.getHasNext();
        }
        set;
    }
    public Boolean hasPrevious {
        get {
            return setCon.getHasPrevious();
        }
        set;
    }
  
    public Integer pageNumber {
        get {
            return setCon.getPageNumber();
        }
        set;
    }
    
    public void first() {
        
        setCon.first();
    }
  
    public void last() {
        
        setCon.last();
    }
  
    public void previous() {
        
        setCon.previous();
    }
  
    public void next() {
        
        setCon.next();
    }   
